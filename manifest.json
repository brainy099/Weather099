{
  "name": "Weather099",
  "version": "0.2",
  "manifest_version": 2,
  "description": "A simple JS weather app",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logo_16.png",
      "32": "images/logo_32.png",
      "48": "images/logo_48.png",
      "128": "images/logo_128.png"
    }
  },
  "permissions": ["activeTab", "storage", "geolocation"],
  "icons": {
    "16": "images/logo_16.png",
    "32": "images/logo_32.png",
    "48": "images/logo_48.png",
    "128": "images/logo_128.png"
  }
}
